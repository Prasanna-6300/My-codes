def subarrays_with_sum(arr, target):
    result = []
    for i in range(len(arr)):
        current_sum = 0
        for j in range(i, len(arr)):
            current_sum += arr[j]
            if current_sum == target:
                result.append(arr[i:j+1])
    return result

print(subarrays_with_sum([1,2,3,4,5], 5))  # [[2,3], [5]]
